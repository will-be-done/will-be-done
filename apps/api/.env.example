# Cloudflare Turnstile CAPTCHA
CF_CAPTCHA_ENABLED=false
CF_CAPTCHA_SITE_KEY=your-site-key
CF_CAPTCHA_SECRET_KEY=your-secret-key

# S3 Backup Configuration
IS_S3_SQLITE_BACKUP_ENABLED=false
S3_ACCESS_KEY_ID=your-access-key
S3_SECRET_ACCESS_KEY=your-secret-key
S3_ENDPOINT=https://s3.us-east-1.amazonaws.com
S3_BUCKET_NAME=will-be-done-backups
S3_REGION=us-east-1

# Backup tiers to enable (comma-separated: hourly,daily,weekly,monthly)
# To disable hourly backups, use: daily,weekly,monthly
BACKUP_ENABLED_TIERS=hourly,daily,weekly,monthly

# Optional: Override retention policy defaults
BACKUP_HOURLY_INTERVAL_HOURS=4
BACKUP_HOURLY_KEEP_COUNT=4
BACKUP_DAILY_KEEP_DAYS=5
BACKUP_WEEKLY_KEEP_WEEKS=2
BACKUP_MONTHLY_KEEP_MONTHS=2
