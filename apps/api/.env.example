# Cloudflare Turnstile CAPTCHA
WBD_CF_CAPTCHA_ENABLED=false
WBD_CF_CAPTCHA_SITE_KEY=your-site-key
WBD_CF_CAPTCHA_SECRET_KEY=your-secret-key

# S3 Backup Configuration
WBD_BACKUP_S3_ENABLED=false
WBD_BACKUP_S3_ACCESS_KEY_ID=your-access-key
WBD_BACKUP_S3_SECRET_ACCESS_KEY=your-secret-key
WBD_BACKUP_S3_ENDPOINT=https://s3.us-east-1.amazonaws.com
WBD_BACKUP_S3_BUCKET_NAME=will-be-done-backups
WBD_BACKUP_S3_REGION=us-east-1

# Backup tiers to enable (comma-separated: hourly,daily,weekly,monthly)
# To disable hourly backups, use: daily,weekly,monthly
WBD_BACKUP_ENABLED_TIERS=hourly,daily,weekly,monthly

# Optional: Override retention policy defaults
WBD_BACKUP_HOURLY_INTERVAL_HOURS=4
WBD_BACKUP_HOURLY_KEEP_COUNT=4
WBD_BACKUP_DAILY_KEEP_DAYS=5
WBD_BACKUP_WEEKLY_KEEP_WEEKS=2
WBD_BACKUP_MONTHLY_KEEP_MONTHS=2
